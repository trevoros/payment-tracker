{
  "disabled": false,
  "bindings": [
    {
      "authLevel": "function",
      "type": "httpTrigger",
      "direction": "in",
      "name": "input",
      "methods": [
        "get"
      ],
      "route": "GetPayments/{state}"
    },
    {
      "type": "cosmosDB",
      "name": "inputDocuments",
      "databaseName": "cardb",
      "collectionName": "car",
      "sqlQuery": "select * from car c where c.state = {state} order by c.name desc",
      "connectionStringSetting": "carreviewstr_COSMOSDB",
      "direction": "in"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    }
  ]
}