{
  "disabled": false,
  "bindings": [
    {
      "authLevel": "function",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "route": "HttpTriggerJS/name/{name}",
      "methods": [
        "get"
      ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "type": "cosmosDB",
      "name": "outputDocument",
      "databaseName": "paymentdb",
      "collectionName": "payment",
      "createIfNotExists": true,
      "connectionStringSetting": "carreviewstr_COSMOSDB",
      "direction": "out",
      "partitionKey": "/name"
    }
  ]
}
